	<queryString>
		<![CDATA[SELECT
      discipulos.disCod
    , discipulos.disnome
    , discipulos.disdiscipulador
    , movimento.movTipo
    , movimento.movValor
    , repasse.resLocal
    , movimento.movData
    , movimento.movProtocolo
FROM
    wwwpais_sistema.celulas
    INNER JOIN wwwpais_sistema.discipulos
        ON (celulas.celDisCod = discipulos.disCod)
    INNER JOIN wwwpais_sistema.movimento
        ON (movimento.celCod = celulas.celCod)
    INNER JOIN wwwpais_sistema.repasse
        ON (repasse.movCod = movimento.movCod) AND (repasse.disCod = discipulos.disCod)
where movimento.movCod = 18]]>
